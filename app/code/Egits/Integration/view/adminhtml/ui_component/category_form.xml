<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<field name="enable_category" sortOrder="10" formElement="checkbox">
		<argument name="data" xsi:type="array">
			<item name="config" xsi:type="array">
				<item name="source" xsi:type="string">
					category
				</item>
				<item name="default" xsi:type="number">
					1
				</item>
			</item>
		</argument>
		<settings>
			<validation>
				<rule name="required-entry" xsi:type="boolean">
					false
				</rule>
			</validation>
			<dataType>
				boolean
			</dataType>
			<label translate="true">
				Show in Slider
			</label>
		</settings>
		<formElements>
			<checkbox>
				<settings>
					<valueMap>
						<map name="false" xsi:type="string">
							0
						</map>
						<map name="true" xsi:type="string">
							1
						</map>
					</valueMap>
					<prefer>
						toggle
					</prefer>
				</settings>
			</checkbox>
		</formElements>
	</field>
